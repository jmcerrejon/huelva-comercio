".no-touch-enable": {
    touchEnabled: false
}

".vNavBar" : {
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
}

".fa" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.fa
    }
}

".fabrands" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.fabrands
    }
}

".fareg" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.fareg
    }
}

".falight" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.falight
    }
}

".ma" : {
    font: {
        fontFamily: "MaterialIcons-Regular"
    }
}

"Window[platform=android]" : {
    theme: "Theme.Custom",
    windowSoftInputMode: Ti.UI.Android.SOFT_INPUT_ADJUST_PAN
}

"ListView" : {
    showVerticalScrollIndicator: false
}

"Window" : {
    navBarHidden: true,
    backgroundColor: "white",
    orientationModes: [Ti.UI.PORTRAIT]
}

"NavigationWindow" : {
    orientationModes: [Ti.UI.PORTRAIT]
}

"Button[platform=android]" : {
    backgroundColor: Alloy.CFG.COLORS.main,
    touchFeedback: true,
    touchFeedbackColor: "#EEEEEE",
}

"TextField" : {
    color: Alloy.CFG.COLORS.dark_gray
}

"TextArea[platform=android]" : {
    color: Alloy.CFG.COLORS.dark_gray,
    backgroundColor: "transparent",
    borderStyle: Ti.UI.INPUT_BORDERSTYLE_NONE
}

"ListItem[platform=ios]" : {
    selectionStyle: Ti.UI.iOS.ListViewCellSelectionStyle.NONE,
    backgroundSelectedColor: "transparent"
}

"SearchBar" : {
    autocapitalization: Ti.UI.TEXT_AUTOCAPITALIZATION_NONE,
    autocorrect: false,
    cancelButtonTitle: "Cancelar",
    hiddenBehavior: Ti.UI.HIDDEN_BEHAVIOR_GONE,
    hintText: "Escriba aqu√≠ para filtrar"
}

"Label" : {
    color: Alloy.CFG.COLORS.main
}

".required" : {
    required: true
}

".txt" : {
    top: 10,
    left: 25,
    right: 25,
    height: 50,
    clearButtonMode: Ti.UI.INPUT_BUTTONMODE_ONFOCUS,
    paddingLeft: 20,
    font: {
        fontFamily: Alloy.CFG.FONTS.light,
        fontSize: 15
    },
    color: Alloy.CFG.COLORS.black,
    borderColor: Alloy.CFG.COLORS.gray,
    borderRadius: 4,
    autocorrect: false,
    autocapitalization: Ti.UI.TEXT_AUTOCAPITALIZATION_NONE
}

".text-center" : {
    textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER
}

".main-color" : {
    color: Alloy.CFG.COLORS.main
}

".no-touch-enable" : {
    touchEnabled: false
}

".keyboard-numeric" : {
    keyboardType: Ti.UI.KEYBOARD_TYPE_NUMBER_PAD,
    autocorrect: false
}

".keyboard-mail" : {
    keyboardType: Ti.UI.KEYBOARD_TYPE_EMAIL,
    autofillType: Ti.UI.AUTOFILL_TYPE_EMAIL,
    autocorrect: false
}

".keyboard-web" : {
    keyboardType: Ti.UI.KEYBOARD_TYPE_URL,
    autofillType: Titanium.UI.AUTOFILL_TYPE_URL,
    autocorrect: false
}

"TextField[platform=android]" : {
    hintTextColor: Alloy.CFG.COLORS.gray,
    backgroundColor: "transparent",
    borderStyle: Ti.UI.INPUT_BORDERSTYLE_NONE
}

".fill-email" : {
    autofillType: Ti.UI.AUTOFILL_TYPE_EMAIL
}

".fill-name" : {
    autofillType: Ti.UI.AUTOFILL_TYPE_NAME
}

".fill-job" : {
    autofillType: Ti.UI.AUTOFILL_TYPE_JOB_TITLE
}

".fill-phone" : {
    autofillType: Ti.UI.AUTOFILL_TYPE_PHONE
}

".fill-website" : {
    autofillType: Ti.UI.AUTOFILL_TYPE_URL
}

"ScrollView[platform=ios]" : {
    keyboardDismissMode: Ti.UI.iOS.KEYBOARD_DISMISS_MODE_ON_DRAG
}

"TabGroup" : {
    navBarHidden: true,
    orientationModes: [Ti.UI.PORTRAIT]
}

".back" : {
    backgroundImage: "/images/common/back.png",
    width: 95,
    left: 0,
    height: 44,
    title: "     Volver",
    color: "black",
    font: {
        fontFamily: Alloy.CFG.FONTS.bold,
        fontSize: "11sp"
    },
    selectedColor: Alloy.CFG.COLORS.main
}

".field" : {
    width: Ti.UI.FILL,
    returnKeyType: Ti.UI.RETURNKEY_NEXT,
    clearButtonMode: Ti.UI.INPUT_BUTTONMODE_ONFOCUS,
    height: 45,
    paddingLeft: 20,
    backgroundColor: "white",
    borderRadius: 3,
    width: Ti.UI.FILL,
    font: {
        fontFamily: Alloy.CFG.FONTS.light,
        fontSize: 15
    },
    color: Alloy.CFG.COLORS.black,
    hintTextColor: "#717171",
    borderColor: Alloy.CFG.COLORS.main2,
    autocorrect: false,
    autocapitalization: Ti.UI.TEXT_AUTOCAPITALIZATION_NONE
}

".field[platform=ios]" : {
    padding: {
        left: 15,
        top: 10,
        bottom: 10,
        right: 15
    },
    tintColor: Alloy.CFG.COLORS.main2
}

".btn" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.light,
        fontSize: 15
    },
    color: "white",
    height: 50,
    backgroundColor: Alloy.CFG.COLORS.main2,
    borderRadius: 5,
    left: "20%",
    right: "20%"
}

".btn-alert" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.light,
        fontSize: 15
    },
    color: "white",
    height: 35,
    backgroundColor: "red",
    borderRadius: 5,
    left: "20%",
    right: "20%"
}

".lblTitleLight" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.light,
        fontSize: 24
    }
}

".lblTitleMedium" : {
    font: {
        fontFamily: Alloy.CFG.FONTS.medium,
        fontSize: 24
    }
}

".tf" : {
    autocorrect: false,
    colorFont: "white",
    colorFocus: Alloy.CFG.COLORS.main2,
    colorDefault: Alloy.CFG.COLORS.main2,
    colorPattern: Alloy.CFG.COLORS.main2,
    returnKey: Ti.UI.RETURNKEY_NEXT
}

".v_container" : {
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE,
    layout: "vertical"
}

".h_container" : {
    width: Ti.UI.SIZE,
    height: Ti.UI.SIZE,
    layout: "horizontal"
}

".separator-half-min" : {
    height: 25
}

".separator-min" : {
    height: 50
}

".separator-medium" : {
    height: 100
}

".separator-max" : {
    height: 200
}

".logoContainer" : {
    width: "20%",
    height: 40
}

".logoImage" : {
    width: 120,
    height: 60,
    backgroundColor: Alloy.CFG.COLORS.white,
    defaultImage: "/images/logo.png",
    brokenLinkImage: "/images/logo.png",
    rounded: false
}

".test": {
    borderColor: "red",
    borderWidth: 2
}